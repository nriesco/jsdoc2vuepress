{
  "name": "jsdoc2vuepress",
  "version": "1.1.3",
  "description": "Simple jsdoc2md wrapper to create ready to use markdown files for vuepress",
  "bin": {
    "jsdoc2vuepress": "./bin/cli.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "DEBUG_LOCAL=true bin/cli.js files/sample.js files/sample.md",
    "patch": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nriesco/jsdoc2vuepress.git"
  },
  "keywords": [
    "vuejs",
    "vuepress",
    "vuepress-plugin",
    "flowchart",
    "jsdocs",
    "jsdoc2md"
  ],
  "author": "Nico R.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nriesco/jsdoc2vuepress/issues"
  },
  "files": [
    "files/header.hbs",
    "bin/cli.js",
    "bin/cleanCoverageFile.js",
    "bin/cleanFlowChart.js"
  ],
  "homepage": "https://github.com/nriesco/jsdoc2vuepress#readme",
  "dependencies": {
    "jsdoc-to-markdown": "^7.0.1"
  }
}
